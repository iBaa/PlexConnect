<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.trash.plist</string>
    <key>OnDemand</key>
    <true/>
    <key>Program</key>
	<string>/usr/bin/trash.bash</string>
	<key>UserName</key>
	<string>root</string>
	<key>RunAtLoad</key>
	<true/>
	<key>WorkingDirectory</key>
	<string>~/usr/bin</string>
    <key>StartInterval</key>
    <integer>300</integer>
</dict>
</plist>
