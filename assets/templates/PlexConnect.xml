<?xml version="1.0" encoding="UTF-8"?>
<atv>
  <head>
    <script src="{{URL(:/js/utils.js)}}" />
  </head>
  <body>
    <viewWithNavigationBar id="PlexConnect_Navigation" onNavigate="loadMenuPage(event)">
      <navigation>
        <navigationItem id="Library">
          <title>{{TEXT(Library)}}</title>
          <url>{{URL(:/library/sections)}}</url>
        </navigationItem>
        <navigationItem id="Channels">
          <title>{{TEXT(Channels)}}</title>
          <url>{{URL(:/channels/all)}}</url>
        </navigationItem>
        <navigationItem id="MyPlex">
          <title>{{TEXT(MyPlex)}}</title>
          <url>{{URL(:/myplex)}}</url>
        </navigationItem>
        <navigationItem id="Settings">
          <title>{{TEXT(Settings)}}</title>
          <url>{{URL(:/&amp;PlexConnect=Settings)}}</url>
        </navigationItem>
      </navigation>
    </viewWithNavigationBar>
  </body>
</atv>
